{
    "nc_key": "s3://ocean-data-bucket/sst/global_sst_2023.nc", 
    "variable_name": "sea_surface_temperature",
    "parquet_key": "local_output/pacific_sst.parquet",
    "filters": [
        {
            "kind": "range",
            "params": {
                "dimension_name": "longitude",
                "min_value": -180.0,
                "max_value": -120.0
            }
        },
        {
            "kind": "range",
            "params": {
                "dimension_name": "latitude", 
                "min_value": -60.0,
                "max_value": 60.0
            }
        },
        {
            "kind": "3d_point",
            "params": {
                "time_dimension_name": "time",
                "lat_dimension_name": "latitude",
                "lon_dimension_name": "longitude",
                "steps": [0.0, 30.0, 60.0, 90.0],
                "points": [
                    [-20.0, -140.0],
                    [0.0, -150.0], 
                    [20.0, -160.0]
                ],
                "tolerance": 2.0
            }
        }
    ]
}