{
    "nc_key": "s3://ocean-data/global/sst_daily_2023.nc",
    "variable_name": "sea_surface_temperature",
    "parquet_key": "examples/output/ocean_processed.parquet",
    "filters": [
        {
            "kind": "range",
            "params": {
                "dimension_name": "longitude", 
                "min_value": -180.0,
                "max_value": -120.0
            }
        },
        {
            "kind": "range",
            "params": {
                "dimension_name": "latitude",
                "min_value": 20.0,
                "max_value": 60.0
            }
        }
    ],
    "postprocessing": {
        "name": "Ocean Data Processing Pipeline", 
        "processors": [
            {
                "type": "RenameColumns",
                "mappings": {
                    "sea_surface_temperature": "sst_kelvin",
                    "latitude": "lat_degree",
                    "longitude": "lon_degree",
                    "time": "day_of_year"
                }
            },
            {
                "type": "UnitConvert",
                "column": "sst_kelvin",
                "from_unit": "kelvin",
                "to_unit": "celsius"
            },
            {
                "type": "ApplyFormula",
                "target_column": "sst_anomaly",
                "formula": "sst_kelvin - 15.0",
                "source_columns": ["sst_kelvin"]
            },
            {
                "type": "ApplyFormula", 
                "target_column": "thermal_front_indicator",
                "formula": "sst_anomaly > 2.0",
                "source_columns": ["sst_anomaly"]
            },
            {
                "type": "DatetimeConvert",
                "column": "day_of_year", 
                "base": "2023-01-01T00:00:00Z",
                "unit": "days"
            }
        ]
    }
}