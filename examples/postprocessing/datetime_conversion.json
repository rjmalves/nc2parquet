{
    "nc_key": "examples/data/pres_temp_4D.nc", 
    "variable_name": "temperature",
    "parquet_key": "examples/output/datetime_converted.parquet",
    "filters": [],
    "postprocessing": {
        "name": "DateTime Conversion Example",
        "processors": [
            {
                "type": "DatetimeConvert",
                "column": "time",
                "base": "1990-01-01T00:00:00Z",
                "unit": "days"
            },
            {
                "type": "RenameColumns",
                "mappings": {
                    "time": "datetime",
                    "temperature": "air_temp"
                }
            }
        ]
    }
}